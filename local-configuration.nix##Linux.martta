{ users, pkgs }:
{
  hostName = "martta";
  grubDevice = "/dev/sda";
  users = with users; [
    (jluttine { groups = [ "wheel" ]; })
    (meri { groups = [ "wheel" ]; })
  ];
  displayManager = "sddm";
  desktopEnvironment = "kde";
  extraServices = {
    syncthing = {
      enable = true;
      user = "meri";
    };
    cryptos.enable = true;
    bluetooth.enable = true;
  };
  allowUnfree = true;
  extraPackages = with pkgs; [
    zoom-us
    linphone

  ];
}
